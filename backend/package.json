{
  "name": "backend",
  "version": "1.0.0",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/app.js",
    "start": "node src/app.js",
    "typeorm": "typeorm",
    "migration:generate": "npx typeorm-ts-node-esm migration:generate ./src/migrations/migration -d ./src/config/data-source.js",
    "migration:run": "npx typeorm migration:run -d ./src/config/data-source.js"
  },
  "dependencies": {
    "@types/node": "^25.0.1",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.28"
  },
  "devDependencies": {
    "nodemon": "^3.1.11",
    "typeorm-ts-node-esm": "^0.3.20"
  }
}
